# Конфигурация базы данных PostgreSQL
DB_HOST=your-database-host.yandexcloud.net
DB_PORT=5432
DB_NAME=calendar_db
DB_USER=calendar_user
DB_PASSWORD=your-secure-password
DB_SSL=true

# API ключ для аутентификации
API_KEY=your-secure-api-key-here

# CORS настройки
ALLOWED_ORIGINS=https://your-domain.com,https://your-telegram-webapp.com

# Настройки приложения
NODE_ENV=production
PORT=3000

# Логирование
LOG_LEVEL=info 

# Rate limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100 